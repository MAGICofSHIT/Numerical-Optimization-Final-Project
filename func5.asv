function f = func5(x, n)
% EG2 function
% x: 输入向量
% n: 向量长度
% 返回值:
%   f: 标量函数值

    % f = (x(1) - 5)^2;
    % for i = 2:n
    %     f = f + (sum(x(1:i)) - 1)^2;
    % end
    % f = 0.5 * sin(x(n)^2);
    % for i = 1:n-1
    %     f = f + sin(x(1) + x(i)^2 - 1);
    % end
    f = 0;
    for i = 1:n-1
        f = f + exp(x(i)) - i * x(i) + 10000 * x(n);
    end
end